<h2>Create new Post</h2>



<%= form_for @post do |f|%>
<%= f.label :title %>
<%= f.text_field :title%>
<br><br>
<%= f.label :content %>
<%= f.text_field :content%>
<br><br>
<%= f.label :destination_id%>
<%= f.collection_select :destination_id, @destinations, :id, :name %>
<br><br>
<%= f.label :blogger_id %>
<%= f.collection_select :blogger_id, @bloggers, :id, :name %>
<br><br>



<%= f.submit %>
<% end %>

